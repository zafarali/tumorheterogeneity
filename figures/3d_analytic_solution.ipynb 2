    "pdf = pf_3d_(f, R, a)\n",
    "not_nans = np.logical_not(np.isnan(np.log10(pdf)))\n",
    "plt.plot(np.log10(f)[not_nans], np.log10(pdf)[not_nans])\n",
    "# plt.plot(np.ones(50)*-3.75, np.linspace(0,18), '--')\n",
